{{#if error}}
    {{#each error}}
        <div class="row">
            <div class="col-lg-4 col-md-4 col-xs-4">
                <h3>Maskine {{_id.machineNumber}}</h3>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-xs-12">
                        {{#each type}}
                            <p>&nbsp;&nbsp;-&nbsp;{{this}}</p>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-xs-4">
                <h3>&nbsp;</h3>
                {{#each timestamp}}
                    <p>{{format this}}</p>
                {{/each}}
            </div>
            <div class="col-lg-4 col-md-4 col-xs-4">
                <h3>&nbsp;</h3>
                {{#each pins}}
                    <p>{{print this}}</p>
                {{/each}}
            </div>
        </div>
    {{/each}}
{{else}}
    <p>Der er ikke nogen indrapportede fejl.</p>
{{/if}}