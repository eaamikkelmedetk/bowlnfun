<div class="container" style="background-color: white;">
<h1>Dagsrapport</h1>
    <div class="row">
        <div class="col-lg-4">
            <h2>Fejltype</h2>
        </div>
        <div class="col-lg-4">
            <h2>Tidspunkt</h2>
        </div>
        <div class="col-lg-4">
            <h2>Pins</h2>
        </div>
    </div>
    {{#each error}}
        <div class="row">
            <div class="col-lg-4">
                <h3>Bane: {{_id.machineNumber}}</h3>
                <div class="row">
                    <div class="col-lg-12">
                        {{#each type}}
                            &nbsp;&nbsp;-&nbsp;{{this}}<br />
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <h3>&nbsp;</h3>
                {{#each timestamp}}
                    {{format this}}<br />
                {{/each}}
            </div>
            <div class="col-lg-4">
                <h3>&nbsp;</h3>
                {{#each pins}}
                    {{this}}<br />
                {{/each}}
            </div>
        </div>
    {{/each}}
</div>