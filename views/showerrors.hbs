<div class="container" style="background-color: white;">
    <h1>Dagsrapport</h1>
    <div class="row">
    <div class="col-lg-3 col-md-3 col-xs-3">
        <div class="input-group input-daterange">
            <input type="text" class="form-control" id="fromDate" value="{{getDate}}">
            <span class="input-group-addon">til</span>
            <input type="text" class="form-control" id="toDate" value="{{getDate}}">
        </div>
    </div>
</div>
    <div class="row">
        <div class="col-lg-4 col-md-4 col-xs-4">
            <h2>Fejltype</h2>
        </div>
        <div class="col-lg-4 col-md-4 col-xs-4">
            <h2>Tidspunkt</h2>
        </div>
        <div class="col-lg-4 col-md-4 col-xs-4">
            <h2>Kegler</h2>
        </div>
    </div>
    {{#each error}}
        <div class="row">
            <div class="col-lg-4 col-md-4 col-xs-4">
                <h3>Bane: {{_id.machineNumber}}</h3>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-xs-12">
                        {{#each type}}
                            &nbsp;&nbsp;-&nbsp;{{this}}<br />
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-xs-4">
                <h3>&nbsp;</h3>
                {{#each timestamp}}
                    {{format this}}<br />
                {{/each}}
            </div>
            <div class="col-lg-4 col-md-4 col-xs-4">
                <h3>&nbsp;</h3>
                {{#each pins}}
                    {{print this}}<br />
                {{/each}}
            </div>
        </div>
    {{/each}}
</div>
